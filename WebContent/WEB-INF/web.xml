<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MeetingManager</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <display-name>CharacterEncodingFilter</display-name>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.mzmy.MeetingManager.filters.CharacterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
    <dispatcher>INCLUDE</dispatcher>
    <dispatcher>ERROR</dispatcher>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>LoginControlFilter</display-name>
    <filter-name>LoginControlFilter</filter-name>
    <filter-class>com.mzmy.MeetingManager.filters.LoginControlFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginControlFilter</filter-name>
    <url-pattern>/yjsppage/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.mzmy.MeetingManager.listeners.LoginCountListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>DeptServlet</display-name>
    <servlet-name>DeptServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.DeptServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeptServlet</servlet-name>
    <url-pattern>/DeptServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegisterServlet</display-name>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PreMeetingServlet</display-name>
    <servlet-name>PreMeetingServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.PreMeetingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PreMeetingServlet</servlet-name>
    <url-pattern>/PreMeetingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AjaxEmpsServlet</display-name>
    <servlet-name>AjaxEmpsServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.AjaxEmpsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxEmpsServlet</servlet-name>
    <url-pattern>/AjaxEmpsServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BookMeetingServlet</display-name>
    <servlet-name>BookMeetingServlet</servlet-name>
    <servlet-class>com.mzmy.MeetingManager.servlets.BookMeetingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookMeetingServlet</servlet-name>
    <url-pattern>/BookMeetingServlet</url-pattern>
  </servlet-mapping>
</web-app>